<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="Usuario">
    <select id="obtenerUsuarios" resultType="model.pojos.Usuario" parameterType="model.pojos.Usuario">
        SELECT * FROM Usuario
    </select>
    
    <select id="login" resultType="model.pojos.Usuario" parameterType="model.pojos.Usuario">
        SELECT * FROM Usuario WHERE username = #{username} AND contrasena = #{contrasena} 
    </select>
    
</mapper>
